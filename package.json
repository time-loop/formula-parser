{
    "name": "@time-loop/hot-formula-parser",
    "version": "3.4.2",
    "description": "Formula parser",
    "type": "commonjs",
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rimraf dist coverage",
        "lint": "eslint src test",
        "lint:fix": "eslint src test --fix",
        "check": "npm run lint && npm run test",
        "test:clear": "jest --clearCache",
        "test": "npm run test:clear && jest --detectOpenHandles",
        "test:coverage": "npm run test:clear && jest --coverage",
        "build": "tsc",
        "generate-parser": "cd src/grammar-parser && jison grammar-parser.jison",
        "release": "generate-release",
        "prepublishOnly": "npm run clean && npm run check && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/time-loop/formula-parser.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "keywords": [
        "formula",
        "formulas",
        "parser",
        "formula-parser",
        "excel",
        "spreadsheet"
    ],
    "engines": {
        "node": ">=18.10.0"
    },
    "devDependencies": {
        "@babel/core": "^7.24.5",
        "@babel/eslint-parser": "^7.24.5",
        "@eslint/js": "^9.2.0",
        "@types/jest": "^29.5.12",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-jest": "^28.5.0",
        "generate-release": "^0.10.2",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "jison": "^0.4.17",
        "jscs": "^3.0.7",
        "rimraf": "^2.3.4",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.9.0"
    },
    "dependencies": {
        "@formulajs/formulajs": "^2.3.0",
        "eslint-plugin-import": "^2.29.1",
        "json5": "^2.2.3",
        "tiny-emitter": "^2.1.0"
    }
}
